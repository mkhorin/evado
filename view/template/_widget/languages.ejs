<!-- _widget/languages -->
<div data-cookie="<%- _module.params.languageCookie %>"
     data-max-age="<%- _widget.cookieMaxAge %>">
<% for (const item of items) { %>
    <a class="dropdown-item <%- item === active ? 'active' : '' %>"
        href="#" data-code="<%- item.code %>"><%- item.label %></a>
<% } %>
</div>