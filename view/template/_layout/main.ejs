<!DOCTYPE html>
<html lang="<%- _controller.language %>">
<head>
  <meta charset="UTF-8">
  <meta content="width=device-width, initial-scale=1" name="viewport">
  <base href="<%- _baseUrl %>">
  <title data-text="<%= blocks.title %>" data-t=""><%- _module.getTitle() %></title>

  <%- include(_view.get('_part/asset/style/main')) %>

  <link rel="stylesheet" href="style/module.css">

  <%- include(_view.get('_part/asset/style/custom')) %>
</head>
<body data-csrf="<%- _controller.getCsrfToken() %>">

<div class="main-wrapper">

  <%- body %>

</div>

<%- include(_view.get('_part/asset/script/main')) %>
<%- include(_view.get('_part/asset/script/messages')) %>
<%- include(_view.get('_part/asset/script/custom')) %>

<script src="script/init.js"></script>

</body>
</html>