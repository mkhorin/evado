<!-- _part/attr/static -->

<% const p = {}; %>
<%- include('_params', {p}) %>
<%
if (p.displayValue && typeof url !== 'undefined') {
    p.displayValue = `<a href="${_url(url)}" class="modal-link">${p.displayValue}</a>`;
}
%>
<div class="form-attr form-group">
    <label class="control-label <%- p.labelCss %>" title="<%= p.extHint %>"
           data-t="<%- p.translate %>"><%- p.label %></label>
    <div class="<%- p.valueCss %>">
        <input name="<%- p.name %>" type="hidden" value="<%= p.value %>">
        <div class="form-control-static">
            <%- p.displayValue %>
        </div>
        <div class="error-block"></div>
    </div>
</div>