<!-- _part/group/inline -->
<%
const p = {
    labelCss: typeof labelCss === 'undefined' ? 'col-lg-2' : labelCss,
    valueCss: typeof valueCss === 'undefined' ? 'col-lg-10' : valueCss
};
%>
<div class="inline-group-container">
    <div class="row">
        <div class="<%- p.labelCss %>"></div>
        <div class="<%- p.valueCss %>">
            <%- include(_view.get('_part/group/columns'), {
                columnCss: typeof columnCss === 'undefined' ? '' : columnCss,
                labelCss: '',
                valueCss: ''
            }) %>
        </div>
    </div>
</div>